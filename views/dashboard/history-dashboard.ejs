<%- include('../partials/dashboard/dashboard-head') %>
						<a href="/admin/userhistories/create" class="btn btn-success" data-toggle="modal"><i class="material-icons">&#xE147;</i> <span>Add New User History</span></a>				
					</div>
				</div>
			</div>
			<table class="table table-striped table-hover">
				<thead>
					<tr>
						<th>ID</th>
						<th>Score</th>
						<th>ID User Game</th>
					</tr>
				</thead>
				<tbody>
					<% userGameHistories.forEach(i => { %>
						<tr>
							<td><%= i.id %></td>
							<td><p><%= i.score %></p></td>
							<td><p><%= i.user_game_id %></p></td>
							<td>
								<a href="/admin/userhistories/update/<%= i.id %>" class="edit" data-toggle="modal"><i class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a>
								<a href="/admin/userhistories/delete/<%= i.id %>" class="delete" data-toggle="modal"><i class="material-icons" data-toggle="tooltip" title="Delete">&#xE872;</i></a>
							</td>
						</tr>
						<% }) %>
<%- include('../partials/dashboard/dashboard-bottom') %>